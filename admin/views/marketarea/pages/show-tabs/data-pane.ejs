                <a data-widgster="collapse" title="Collapse" href="#"><i class="glyphicon glyphicon-minus"></i></a>
                <a data-widgster="close" title="Close" href="#"><i class="glyphicon glyphicon-remove"></i></a>
            </div>
        </header>
        <div class="body">
            <h4></h4>
            <table class="table table-striped table-hover" id="ctpp-table">
                <tbody>
                </tbody>
                <!--<tr ng-repeat="var in current_vars">
                    <td ng-click="colorMap(var)">{{census_vars[var].name}}</td>
                    <td>{{census_vars[var].value}}</td>
                </tr>-->
            </table>
        </div>
    </div>

    <div ng-show="setActiveOverviewTab('LODES')" id="lodes-data-pane-div">
